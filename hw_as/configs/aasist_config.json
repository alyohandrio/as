{
  "name": "basic_train",
  "n_gpu": 1,
  "arch": {
    "type": "AASIST",
      "args": {
      "sinc_channels": 70,
      "sinc_kernel": 128,
      "res_channels": [[1, 32], [32, 32], [32, 64], [64, 64], [64, 64], [64, 64]],
      "gat_ks": [0.5, 0.7],
      "gat_dropout": 0.2,
      "pool_dropout": 0.2,
      "mgo_in_dims": [64, 32],
      "mgo_out_dims": [32, 32],
      "mgo_ks": [[0.5, 0.5], [0.5, 0.5]],
      "feat_dropout": 0.2,
      "final_dropout": 0.5
    }
  },
  "data": {
    "train": {
      "batch_size": 8,
      "num_workers": 2,
      "datasets": [
        {
          "type": "ASVDataset",
          "args": {
            "root": ".",
	    "split": "train",
	    "l_suf": "train.trn"
          }
        }
      ]
    },
    "val": {
      "batch_size": 32,
      "num_workers": 2,
      "datasets": [
        {
          "type": "ASVDataset",
	  "args": {
            "root": ".",
	    "split": "eval",
	    "l_suf": "eval.trl"
	  }
	}
      ]
    }
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 1e-4,
      "betas": [0.8, 0.99],
      "weight_decay": 1e-2
    }
  },
  "loss": {
    "type": "CELoss",
    "args": {}
  },
  "metrics": [
    {
      "type": "EER",
      "args": {}
    }
  ],
  "lr_scheduler": {
    "args": {
      "T_max": 40000
    },
    "type": "CosineAnnealingLR"
  },
  "trainer": {
    "epochs": 1000,
    "monitor": "min val_loss",
    "save_dir": "saved/",
    "len_epoch": 100,
    "verbosity": 2,
    "visualize": "wandb",
    "early_stop": 100,
    "save_period": 10,
    "wandb_project": "as_test",
    "grad_norm_clip": 10
  }
}
